package aula3;

public class Livros {
	String nome;
	String autor;
	String genero;
	String tipoDeMidia;
	String anoDePublicacao;
	String editora;
	String prefacio;
	String idioma;
	String isbn;
	String edicao;
	
	
	void exibirLivro() {
		System.out.println(nome);
		System.out.println(autor);
		System.out.println(genero);
		System.out.println(tipoDeMidia);
		System.out.println(anoDePublicacao);
		System.out.println(editora);
		System.out.println(prefacio);
		System.out.println(idioma);
		System.out.println(isbn);
		System.out.println(edicao);
		
	}

}
